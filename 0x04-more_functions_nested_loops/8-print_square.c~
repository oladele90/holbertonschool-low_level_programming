#include "main.h"

/**
 * print_square - prints square
 *@size: size of square to be printed
 * Return: nothing
 */

void print_square(int size)
{
	int i = size, m = size;
	if (size <= 0)
		_putchar('\n');
	while (size >= 1)
	{
		while (i >= 1)
		{
			_putchar('#');
			i--;
		}
		_putchar('\n');
		size--;
		i = m;
	}
}
